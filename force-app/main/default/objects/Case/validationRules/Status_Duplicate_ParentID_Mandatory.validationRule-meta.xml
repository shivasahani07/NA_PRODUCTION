<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Status_Duplicate_ParentID_Mandatory</fullName>
    <active>true</active>
    <errorConditionFormula>and (text( Status ) = &apos;Duplicate&apos; ,  ISBLANK( ParentId ) )</errorConditionFormula>
    <errorDisplayField>ParentId</errorDisplayField>
    <errorMessage>Parent Case is mandatory</errorMessage>
</ValidationRule>
